<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="indi.xezzon.school.eams.repository.CourseScheduleMapper">

    <select id="queryByCourseId" resultType="indi.xezzon.school.common.model.CourseSchedule">
        SELECT *
        FROM course_schedule
        WHERE course_id = #{courseId}
    </select>
</mapper>
